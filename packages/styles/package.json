{
  "name": "@livekit/components-styles",
  "version": "0.1.0",
  "author": "LiveKit",
  "license": "MIT",
  "main": "dist/styles.css",
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "dist",
    "src"
  ],
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "dev": "nodemon -e scss -x \"yarn build && yarn generate:types\"",
    "build": "sass src/styles.scss dist/styles.css && yarn generate:types",
    "generate:types": "cd src && typed-scss-modules './**/*.scss' --exportType default --outputFolder ../dist/types --nameFormat kebab"
  },
  "peerDependencies": {},
  "devDependencies": {
    "modern-normalize": "^1.1.0",
    "nodemon": "^2.0.19",
    "sass": "^1.54.6",
    "typed-scss-modules": "^6.5.0"
  },
  "dependencies": {}
}
