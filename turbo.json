{
  "$schema": "https://turborepo.org/schema.json",
  "baseBranch": "origin/main",
  "pipeline": {
    "build": {
      "dependsOn": [
        "^build"
      ],
      "outputs": [
        "dist/**",
        ".next/**",
        "storybook-static/**"
      ]
    },
    "lint": {
      "outputs": []
    },
    "test": {
      "dependsOn": [
        "^build"
      ],
      "outputs": []
    },
    "test:build": {
      "outputs": []
    },
    "test:unit": {
      "outputs": []
    },
    "test:watch": {
      "outputs": []
    },
    "dev": {
      "cache": false,
      "dependsOn": [
        "^dev"
      ]
    },
    "deploy": {
      "dependsOn": [
        "^build"
      ]
    },
    "gen:docs": {
      "cache": true,
      "dependsOn": [
        "^build"
      ]
    }
  }
}